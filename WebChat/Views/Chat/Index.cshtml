@model IEnumerable<string>
@{
    ViewData["Title"] = "Chat";
}
страница сгенерирована: @DateTime.Now (AJAX proof)
<h1>Чат</h1>

@await Component.InvokeAsync("NewMessageForm")

<div class="text-left">
    <textarea disabled id="messages" cols="100" rows="15" >
        @foreach (var item in Model)
